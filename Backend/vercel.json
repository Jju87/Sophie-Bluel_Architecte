{
    "version": 2,
    "name": "Sophie-Bluel",
    "builds": [
       { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/api/users/(.*)", "dest": "/api/users/$1" },
       { "src": "/api/categories/(.*)", "dest": "/api/categories/$1" },
       { "src": "/api/works/(.*)", "dest": "/api/works/$1" },
       { "src": "/api-docs/(.*)", "dest": "/api-docs/$1" }
    ]
}